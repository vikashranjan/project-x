<launch>

 <!-- Cameras -->

 <include file="$(find realsense2_camera)/launch/rs_rgbd.launch">
    <arg name="camera" value="d435_1" />
     <arg name="serial_no"         default="806312060532"/>
  </include>

  <include file="$(find realsense2_camera)/launch/rs_rgbd.launch">
    <arg name="camera" value="d435_2" />
     <arg name="serial_no"         default="916512061256"/>
  </include>


<node pkg="tf" type="static_transform_publisher" name="base_to_d435_1_tf"
      args="0.16 0.0 0.09 0.0 0.0 0.0 base_link d435_1_link 100" />

  <node pkg="tf" type="static_transform_publisher" name="base_to_d435_2_tf"
      args="0.08 0.0 0.535 0.0 0.0 0.0 base_link d435_2_link 100" />

</launch>
